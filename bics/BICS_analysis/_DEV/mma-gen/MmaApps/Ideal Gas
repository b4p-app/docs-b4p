12}, {-12, 12}};:[font = input; wordwrap; dontPreserveAspect; endGroup]BouncePoint3[startingPoint, walls, 100]:[font = section; inactive; dontPreserveAspect; startGroup]Implementation:[font = text; inactive; dontPreserveAspect]Note: To save file space, this animation is stored in Bitmap PICT form, rather than resolution-independent PostScript.  You can regenerate the PostScript by running the Mathematica code given here.  Note, however, that this may take a long time, and may require a great deal of memory.;[s]3:0,0;169,1;180,0;286,-1;2:2,13,9,Times,0,12,0,0,0;1,13,9,Times,2,12,0,0,0;:[font = text; inactive; Cclosed; dontPreserveAspect; startGroup]Define BounceOffWall::[font = input; wordwrap; dontPreserveAspect; endGroup]BounceOffWall[{x_, v_}, xMin_, xMax_] :=     Block[{lx = x + v},       If[lx > xMax, {2 xMax - lx, -v}, 		  If[lx < xMin, {2 xMin - lx, -v}, 						{lx, v}]]     		 ];:[font = text; inactive; Cclosed; dontPreserveAspect; startGroup]Define NextStep::[font = input; wordwrap; dontPreserveAspect; endGroup]NextStep3[thePoint_List, 		 boundries:{{xMin_, xMax_}, {yMin_, yMax_}, 		 			{zMin_, zMax_}}] := 	Map[{BounceOffWall[#[[1]], xMin, xMax],		 BounceOffWall[#[[2]], yMin, yMax],		 BounceOffWall[#[[3]], zMin, zMax]}&,		 thePoint];:[font = text; inactive; Cclosed; dontPreserveAspect; startGroup]Define BouncePoint3::[font = input; wordwrap; dontPreserveAspect; endGroup; endGroup; endGroup]BouncePoint3[initialPoint_List,           boundries:{{xMin_, xMax_},                      {yMin_, yMax_},                      {zMin_, zMax_}},           numberOfSteps_] :=Block[{currentPoint = initialPoint},Do[  Show[Graphics3D[ 	 {PointSize[0.03], 	  Map[Point[{#[[1]][[1]], #[[2]][[1]], 	  			 #[[3]][[1]]}]&, currentPoint]}, 		  PlotRange->{{xMin, xMax}, {yMin, yMax},		  			  {zMin, zMax}},		  BoxRatios->Automatic]];  currentPoint = NextStep3[currentPoint, boundries],  {numberOfSteps}  ]]^*)